  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example @2",
      "program": "${workspaceFolder}/tt.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "example/example.js"
      ],
      // "skipFiles": [
      //   "<node_internals>/**/*.js"
      // ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Testt",
      "program": "${workspaceFolder}/build/depack.js",
      "args": [
        "src/bin/depack.js",
        "-c", "-n", "-I", "2018", "-O", "2017", "-a", "-o",
        "depack1.js", "-w"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Frontend",
      "program": "${workspaceFolder}/src/bin/index.js",
      "args": [
        "frontend-admin/index.jsx",
        "-o", "build/admin.js", "-I", "2018", "-a",
        "-H", "--charset=UTF-8"
      ],
      "cwd": "${workspaceFolder}/../../artdecoweb/knedv.ru",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Transform",
      "program": "${workspaceFolder}/output/transform.js",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "example/example.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Compiled",
      "program": "${workspaceFolder}/out/example2.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        // "example/example.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Depack",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "src/cli/index.js",
        "-a",
        //  "out/example2.js",
      ],
      "cwd": "${workspaceFolder}/../../Downloads/kibana-6.5.0-SNAPSHOT-darwin-x86_64",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Compile",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": {"DEBUG": "1"},
      "args": [
        "src/bin/depack.js",
        // "t/wrote/index.js",
        "-c",
        "-n",
        "-I", "2018",
        "-O", "2017"
        "-o", "depack.js",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
