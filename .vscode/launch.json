{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": { "NODE_DEBUG": "depack" },
      "console": "integratedTerminal",
      "args": [
        "test/spec", "test/mask",
        "-a", "-w",
        "-t", "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Depack Build",
      "program": "${workspaceFolder}/compile/depack.js",
      "args": [
        "src/depack",
        "-c", "-a",
        "-o", "depack1.js"
      ],
      "env": { "DEBUG": "1" },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Frontend",
      "program": "${workspaceFolder}/src/bin/index.js",
      "args": [
        "frontend-admin/index.jsx",
        "-o", "build/admin.js", "-a",
        "-H", "--charset=UTF-8"
      ],
      "cwd": "${workspaceFolder}/../../artdecoweb/knedv.ru",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "example/example.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Depack",
      "program": "${workspaceFolder}/src/index.js",
      "env": {
        "NODE_DEBUG": "depack",
      },
      "console": "integratedTerminal",
      "args": [
        "src/index.js",
        "-a", "-c",
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Compile",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": {"DEBUG": "1"},
      "args": [
        "src/bin/depack.js",
        "-c", "-a",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Typal",
      "program": "${workspaceFolder}/../../depack/depack/src/bin/index.js",
      "env": {"GOOGLE_CLOSURE_COMPILER": "/Volumes/backup/closure-compiler/target/closure-compiler-1.0-SNAPSHOT.jar"},
      "args": [
        "src/bin/typal.js",
        "-c", "-o", "build/bin", "-a", "-p", "-S",
      ],
      "cwd": "${workspaceFolder}/../../adc/typal",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Debug",
      "program": "${workspaceFolder}/../../depack/depack/src/bin/index.js",
      "env": {"GOOGLE_CLOSURE_COMPILER": "/Volumes/backup/closure-compiler/target/closure-compiler-1.0-SNAPSHOT.jar"},
      "args": [
        "depack", "--externs", "externs.js",
        "-o", "depack/index_.js", "-a", "-p", "-l"
      ],
      "cwd": "${workspaceFolder}/../../idiocc/debug",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Koa",
      "program": "${workspaceFolder}/../../depack/depack/src/bin/index.js",
      "env": {"GOOGLE_CLOSURE_COMPILER": "/Volumes/backup/closure-compiler/target/closure-compiler-1.0-SNAPSHOT.jar"},
      "args": [
        "tt/t",
        "-o", "tt/t_.js", "-a", "-p", "-c",
        "--process_common_js_modules"
      ],
      "cwd": "${workspaceFolder}/../../idiocc/koa",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Ã€lamode",
      "program": "${workspaceFolder}/../../depack/depack/src/bin/index.js",
      "env": {"GOOGLE_CLOSURE_COMPILER": "/Volumes/backup/closure-compiler/target/closure-compiler-1.0-SNAPSHOT.jar"},
      "args": [
        "src/bin/alamode",
        "-a", "-p", "-c",
      ],
      "cwd": "${workspaceFolder}/../../a-la/alamode",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Splendid StdLib",
      "program": "${workspaceFolder}/src/index.js",
      "args": [
        "src/stdlib",
        "-o", "stdlib/index.js", "-a", "-c", "-p", "-s",
        "-O", "2018", "--source_map_include_content"
      ],
      "cwd": "${workspaceFolder}/../../adc/splendid",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Static Analysis",
      "program": "${workspaceFolder}/src/index.js",
      "args": [
        "src/depack",
        "-o", "compile", "-a", "-p", "-c", "-s", "--source_map_include_content"
        // "src/stdlib",
        // "-o", "stdlib/index.js", "-a", "-c", "-p", "-s",
        // "-O", "2018", "--source_map_include_content"
      ],
      "cwd": "${workspaceFolder}/../static-analysis",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
